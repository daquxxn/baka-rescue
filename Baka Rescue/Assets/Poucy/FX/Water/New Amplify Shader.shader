			float2 coords2 = i.uv_texcoord * 1.0;
			float2 id2 = 0;
			float voroi2 = voronoi2( coords2, id2 );
			float4 color13 = IsGammaSpace() ? float4(0.5235849,1,0.9999999,0) : float4(0.236524,1,0.9999997,0);
			o.Emission = ( pow( voroi2 , 1.5 ) * color13 ).rgb;
			float3 temp_cast_2 = (10.0).xxx;
			o.Translucency = temp_cast_2;
			o.Alpha = 1;
		}

		ENDCG
